    <PROCESSING>
        <SolutionProcessing>
            <ChooseParameter>
                <Solvent>
                    <SolventName>acetone</SolventName>
                    <SolventAmount>
                        <description>Since it is difficult to vaporize only a limited amount of the acetone was added into the resin.</description>
                    </SolventAmount>
                </Solvent>
            </ChooseParameter>
            <ChooseParameter>
                <Additive>
                    <Description>(resin + hardener:BYK = 10:0.2)</Description>
                    <Additive>BYK-9076 surfactant</Additive>
                </Additive>
            </ChooseParameter>
            <ChooseParameter>
                <Mixing>
                    <Description>A Teflon rod was used to manually stir the mixture</Description>
                    <MixingMethod>stirring</MixingMethod>
                    <Time>
                        <value>5</value>
                        <unit>minutes</unit>
                    </Time>
                </Mixing>
            </ChooseParameter>
            <ChooseParameter>
                <Mixing>
                    <Description>Solution was added to a mortar and stirred manually.</Description>
                    <MixingMethod>stirring</MixingMethod>
                    <Time>
                        <value>10</value>
                        <unit>minutes</unit>
                    </Time>
                </Mixing>
            </ChooseParameter>
            <ChooseParameter>
                <Mixing>
                    <Description>Tip sonication was conducted at 10-15 W to avoid overheating</Description>
                    <Mixer>Sonicator 3000 machine</Mixer>
                    <MixingMethod>ultra-sonication</MixingMethod>
                    <Time>
                        <value>6</value>
                        <unit>hours</unit>
                    </Time>
                </Mixing>
            </ChooseParameter>
            <ChooseParameter>
                <Mixing>
                    <Description>bath sonication continued for 6 hrs at full power</Description>
                    <MixingMethod>ultra-sonication</MixingMethod>
                    <Time>
                        <value>6</value>
                        <unit>hours</unit>
                    </Time>
                </Mixing>
            </ChooseParameter>
            <ChooseParameter>
                <Drying-Evaporation>
                    <Description>A full vacuum was applied to remove all solvents and trapped air</Description>
                    <Temperature>
                        <value>75</value>
                        <unit>Celsius</unit>
                    </Temperature>
                    <Time>
                        <value>1</value>
                        <unit>hours</unit>
                    </Time>
                    <AmbientCondition>vacuum</AmbientCondition>
                </Drying-Evaporation>
            </ChooseParameter>
            <ChooseParameter>
                <Cooling>
                    <Description>The mixture was subsequently cooled to rt</Description>
                </Cooling>
            </ChooseParameter>
            <ChooseParameter>
                <Mixing>
                    <Description>The hardener was added to the mixture and manually stirred with a Teflon rod</Description>
                    <MixingMethod>stirring</MixingMethod>
                    <ChemicalUsed>
                        <description>hardener</description>
                    </ChemicalUsed>
                    <Time>
                        <value>5</value>
                        <unit>minutes</unit>
                    </Time>
                </Mixing>
            </ChooseParameter>
            <ChooseParameter>
                <Molding>
                    <MoldingMode>hot-pressing</MoldingMode>
                    <MoldingInfo>
                        <Description>Mixture was cast into mold and cured in an oven</Description>
                        <Temperature>
                            <value>60</value>
                            <unit>Celsius</unit>
                        </Temperature>
                        <Time>
                            <value>2</value>
                            <unit>hours</unit>
                        </Time>
                    </MoldingInfo>
                </Molding>
            </ChooseParameter>
            <ChooseParameter>
                <Molding>
                    <MoldingMode>hot-pressing</MoldingMode>
                    <MoldingInfo>
                        <Description>Samples were post cured with a ramping speed of 0.4 C/min</Description>
                        <Temperature>
                            <value>93.3</value>
                            <unit>Celsius</unit>
                        </Temperature>
                        <Time>
                            <value>4</value>
                            <unit>hours</unit>
                        </Time>
                    </MoldingInfo>
                </Molding>
            </ChooseParameter>
            <ChooseParameter>
                <Other>Samples were polished and prepared for analysis</Other>
            </ChooseParameter>
